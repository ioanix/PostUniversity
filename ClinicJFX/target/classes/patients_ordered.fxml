<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="406.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PatientController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <children>
        <TableView fx:id="tblPatientsOrdered" prefHeight="406.0" prefWidth="361.0" GridPane.rowIndex="1">
            <columns>
                <TableColumn fx:id="categoryColumn" prefWidth="213.0" text="Category">
                    <cellValueFactory>
                        <PropertyValueFactory property="category" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="numberOfPatientsColumn" prefWidth="146.0" text="Number of patients">
                    <cellValueFactory>
                        <PropertyValueFactory property="numberOfPatients" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
      <Label />
    </children>
</GridPane>
